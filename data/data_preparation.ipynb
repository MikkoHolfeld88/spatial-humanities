   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "74bdc4bd-8133-4eb1-982e-2eb63d782387",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Einlesen der Exceldatei\n",
    "hospitals_df=pd.read_excel('data/hospital_data_3.xlsx') "
