   "id": "1f10a672-8a48-4d95-981b-5ce4467575de",
   "metadata": {},
   "outputs": [],
   "source": [
    "#auf sächsische Einrichtungen filtern\n",
    "hospitals_df= hospitals_df[hospitals_df['Land'] == 14].reset_index(drop=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b70f81f5-b889-4c0b-90c1-a4cd2bbe8eac",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Einheitliche Konvertierung zu String und Entfernen unsichtbarer Zeichen\n",
    "hospitals_df.columns = hospitals_df.columns.astype(str)\n",
    "hospitals_df.columns = hospitals_df.columns.str.strip()\n",
    "\n",
    "#Auswählen und umbenennen der relevanten Spalten\n",
    "new_columns = {\n",
    "\t'Land':'Land',\n",
    "\t'Kreis':'Kreis',\n",
    "\t'Gemeinde':'Gemeinde',\n",
    "\t'Adresse_Name':'Name_Einrichtung',\n",
    "\t'Adresse_Strasse_Standort':'Adresse_Strasse_Standort',\n",
    "\t'Adresse_Haus-Nr._Standort':'Adresse_Haus-Nr._Standort',\n",
    "\t'Adresse_Ort_Standort':'Adresse_Ort_Standort',\n",
    "    'Traeger':'Traeger',\n",
    "\t'EinrichtungsTyp':'EinrichtungsTyp',\n",
    "\t'Allgemeine_Notfallversorgung':'Allgemeine_Notfallversorgung',\n",
    "\t'INSG':'INSG',\n",
    "\t'0100':'Betten innere Medizin',\n",
    "    '0300':'Betten Kardiologie ',\n",
    "    '1500':'Betten Allgemeine Chirurgie',\n",
    "\t'2100':'Betten Herzchirurgie',\n",
    "\t'2200':'Betten Urologie',\n",
    "\t'2600':'Betten HNO',\n",
    "\t'2900':'Betten Psychatrie'\n",
    "}\n",
    "hospitals_df = hospitals_df[list(new_columns.keys())].rename(columns=new_columns)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "b77bca2d-11a2-4a77-a5c4-281fba9116e2",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Hinzufügen der Adresse bestehend aus 3 Spalten\n",
    "hospitals_df['Adresse'] = hospitals_df.apply(lambda row: ' '.join(map(str, row[['Adresse_Strasse_Standort', 'Adresse_Haus-Nr._Standort', 'Adresse_Ort_Standort']])), axis=1)\n",
    "\n",
    "# Alte Spalten löschen\n",
    "hospitals_df.drop(columns=['Adresse_Strasse_Standort', 'Adresse_Haus-Nr._Standort', 'Adresse_Ort_Standort'], inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "8e3b99ae-1bbb-4414-91cf-6fd048fbcc4b",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Display(hospitals_df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "dd1e3f56-bb9c-43ee-aee9-47a1d969b52e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Iteriere über jede Adresse und füge die Geo-Koordinaten ein\n",
    "for index, row in hospitals_df.iterrows():\n",
    "    adresse = row['Adresse']\n",
    "    try:\n",
    "        location_point = ox.geocode(adresse)\n",
    "        point = Point(location_point[1], location_point[0])  # Point(longitude, latitude)\n",
    "        hospitals_df.at[index, 'Koordinate'] = point\n",
    "    except:\n",
    "        hospitals_df.at[index, 'Koordinate'] = None"
   ]
  },
